{
    "KDE-KIO-Protocols": {
        "sftp": {
            "Class": ":internet",
            "Icon": "folder-remote",
            "X-DocPath": "kioworker6/sftp/index.html",
            "copyFromFile": true,
            "copyToFile": true,
            "deleting": true,
            "input": "none",
            "linking": true,
            "listing": [
                "Name",
                "Type",
                "Size",
                "Date",
                "AccessDate",
                "Access",
                "Owner",
                "Group",
                "Link",
                "MimeType"
            ],
            "makedir": true,
            "maxInstances": 20,
            "maxInstancesPerHost": 5,
            "moving": true,
            "opening": true,
            "output": "filesystem",
            "protocol": "sftp",
            "reading": true,
            "truncating": true,
            "writing": true
        }
    }
}
